{
  "name": "nodepop",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "lint": "standard --fix",
    "dev": "cross-env DEBUG=nodepop:* nodemon",
    "clean_db": "node clean_db.js",
    "test": "npm run lint && npm run e2e",
    "e2e": "cross-env LOG_FORMAT=nolog mocha **/e2e/*.e2e.js --recursive --timeout 120000",
    "e2ew": "cross-env LOG_FORMAT=nolog mocha **/e2e/*.e2e.js --recursive --watch --timeout 120000"
  },
  "dependencies": {
    "array-shuffle": "^1.0.1",
    "axios": "^0.16.2",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "ejs": "~2.5.6",
    "express": "~4.15.2",
    "hash.js": "^1.1.3",
    "i18n": "^0.8.3",
    "mongoose": "^4.11.12",
    "morgan": "~1.8.1",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "cross-env": "^5.1.1",
    "mocha": "^4.0.1",
    "mockgoose": "^7.3.3",
    "nodemon": "^1.12.1",
    "standard": "^10.0.3",
    "supertest": "^3.0.0"
  }
}
